	p.then 相当于
	new Promise((resolve, reject) => {
		const p1 = new Promise((resolve, reject) => {
			setTimeout(() => {
				resolve(1)
			}, 0)
			resolve(2)
		})
		p1.then(res => {
			console.log(res)
		})
